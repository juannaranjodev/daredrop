{
  "name": "root",
  "version": "1.0.0",
  "main": "/",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "scripts": {
    "makeFilenamesFile": "touch ./webpackCompressedFilenames.js && echo export default [] > ./webpackCompressedFilenames.js",
    "removeDistFolder": "rm -rf ./dist",
    "deployPrepare": "yarn makeFilenamesFile && yarn removeDistFolder",
    "configPrepare": "mkdir -p config && touch ./config/cfOutput-Dev.js && touch ./config/cfOutput-Production.js && touch ./config/cfOutput-Testing.js && touch ./config/cfOutput-Staging.js",
    "test": "./node_modules/.bin/jest",
    "start": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --content-base build --inline --config webpack.config.js --port 8585",
    "buildTemplate": "./node_modules/.bin/babel-node ./src/aws/util/buildTemplate --presets=env,stage-0 --plugins=package-name-import > cfTemplate.json",
    "deploy": "yarn deployPrepare && ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli deploy",
    "update": "yarn deployPrepare && ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli update",
    "delete": "./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli remove",
    "outputs": "./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli outputs",
    "createDdb": "docker run --name dynamodb -p 9000:8000 amazon/dynamodb-local",
    "startDdb": "docker start dynamodb",
    "build": "yarn removeDistFolder && STAGE=staging webpack --config webpack.config.js -p",
    "bundleLambda": "rm -rf ./.cloudFormation && ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli bundleLambda",
    "genAWSCreds": "./node_modules/.bin/babel-node --presets=env ./customScripts/generateAWSCredentials.js",
    "runLambda": "./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli bundleLambda && docker run -v $PWD/.cloudFormation/:/var/task --env-file ./AWSCredentials.txt awslambda-node dareMeDevApiLambdaFunction.default",
    "lmb": "./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli bundleLambda && docker run -v $PWD/.cloudFormation/:/var/task lmb dareMeXDevApiLambdaFunction.default",
    "deployProduction": "yarn deployPrepare && STAGE=production ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli deploy -s production",
    "updateProduction": "yarn deployPrepare && STAGE=production ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli update -s production"
  },
